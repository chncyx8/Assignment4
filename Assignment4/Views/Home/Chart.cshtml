@model IEnumerable<Assignment4.Models.EF_model.Chart>

@{
    ViewData["Title"] = "Chart";
}

<h2>Chart - Period of 5 Years</h2>


<table class="table">
    <thead>
        <tr>
            <th>
                Date
            </th>
            <th>
                Change
            </th>
            <th>
                Change Over Time
            </th>
            <th>
                Change Percent
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.label)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.change)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.changeOverTime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.changePercent)
                </td>
                <td></td>
            </tr>
        }
    </tbody>
</table>