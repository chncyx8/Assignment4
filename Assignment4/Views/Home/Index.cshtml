@{
    ViewData["Title"] = "Home Page";
}

@model IEnumerable<Assignment4.Models.EF_model.Company>

<h2>Symbols</h2>
<hr />

<table class="table">
    <thead>
        <tr>
            <th>
                Symbol
            </th>
            <th>
                Company Name
            </th>  
            <th>
                Financials
            </th>
            <th>
                ShortInterestList
            </th>
           
        </tr>
    </thead>

    <tbody>

        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.symbol)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                <a asp-controller="Home" asp-route-id="@item.symbol" asp-action="Financials">Financials</a>
            </td>
            <td>
                <a asp-controller="Home" asp-route-id="@item.symbol" asp-action="ShortInterestList">ShortInterestList</a>
            </td>
        </tr>
        }
    </tbody>
</table>

<a asp-controller="Home" asp-action="AboutUs">About Us</a>



